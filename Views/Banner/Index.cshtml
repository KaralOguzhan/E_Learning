@model List<MyElearningProject.DAL.Entities.QuadLink>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<div class="container">

    <div class="row g-4">

        @foreach (var item in Model)
        {


            <div class="col-lg-3 col-sm-6 wow fadeInUp" data-wow-delay="0.1s">
                <div class="service-item text-center pt-3">
                    <a href="/Banner/updateQuads/@item.quadLinkID"  class="p-4">
                        <i class="fa fa-3x @item.Ikon text-primary mb-4"></i>
                        <h5 class="mb-3">@item.Title</h5>
                        <p>@item.Description</p>
                    </a>
                </div>
            </div>
        }
    </div>
</div>